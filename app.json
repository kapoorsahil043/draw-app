{
  "expo": {
    "name": "my-draw",
    "slug": "my-draw",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rewards_icon.png",
    "splash": {
      "image": "./assets/rewards_icon_hover.png",
      "backgroundColor": "#f40105",
      "resizeMode": "native"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rewards_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "l.d",
      "useNextNotificationsApi":true
    },
    "web": {
      "favicon": "./assets/rewards_icon.png"
    }
  }
}
